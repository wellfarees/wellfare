{
  "name": "wellfare-backend",
  "version": "0.1.0",
  "description": "Backend for Wellfare.",
  "main": "./dist/src/main.js",
  "scripts": {
    "check-types": "tsc --noemit",
    "clean": "rimraf dist/",
    "create-public-dir": "cd dist/src && mkdirp images",
    "setup": "npm install && prisma generate && tsc",
    "build": "npm run clean && tsc && npm run create-public-dir",
    "start": "node .",
    "dev": "nodemon src/main.ts",
    "lint": "eslint --ext .ts ./src",
    "lint:fix": "eslint src/. --fix",
    "typegen": "graphql-codegen --config codegen.yml",
    "test": "uvu -r tsm tests"
  },
  "license": "MIT",
  "devDependencies": {
    "@graphql-codegen/cli": "2.16.1",
    "@graphql-codegen/introspection": "2.2.3",
    "@graphql-codegen/typescript": "2.8.5",
    "@graphql-codegen/typescript-resolvers": "2.7.10",
    "@types/bcrypt": "^5.0.0",
    "@types/cron": "^1.7.3",
    "@types/glob": "^7.2.0",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.11.11",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.3.0",
    "@urql/core": "^2.3.5",
    "babel-eslint": "8",
    "copyfiles": "^2.4.1",
    "cross-fetch": "^3.1.4",
    "eslint": "^8.30.0",
    "mkdirp": "^1.0.4",
    "nodemon": "^2.0.20",
    "prisma": "^3.6.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "tsm": "^2.2.1",
    "typescript": "^4.5.2",
    "uvu": "^0.5.2"
  },
  "dependencies": {
    "@apollo/server": "^4.3.0",
    "@prisma/client": "^3.6.0",
    "@sendgrid/mail": "^7.6.0",
    "@types/express": "^4.17.13",
    "@types/graphql-upload": "^8.0.9",
    "@types/imagemin": "^8.0.0",
    "@types/imagemin-jpegtran": "^5.0.1",
    "@types/imagemin-pngquant": "^8.0.0",
    "@types/inline-css": "^3.0.1",
    "algoliasearch": "^4.11.0",
    "apollo-datasource": "^3.0.3",
    "apollo-server-core": "^3.5.0",
    "apollo-server-express": "^3.5.0",
    "aws-sdk": "^2.1058.0",
    "axios": "^0.25.0",
    "bcrypt": "^5.0.1",
    "cron": "^1.8.2",
    "css-inline": "^0.8.4",
    "date-fns": "^2.27.0",
    "dotenv": "^8.6.0",
    "express": "^4.17.2",
    "glob": "^7.2.0",
    "graphql": "^16.2.0",
    "graphql-upload": "^13.0.0",
    "imagemin": "7.0.1",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-pngquant": "^9.0.2",
    "inline-css": "^4.0.1",
    "jsonwebtoken": "^8.5.1"
  },
  "overrides": {
    "parse5-htmlparser2-tree-adapter": "^7.0.0"
  },
  "engines": {
    "node": "16.x"
  }
}
